{
    "manifest_version": 3,
    "name": "Tal Bener Hello World",
    "version": "1.0",
    "description": "tbener extension",
    "content_scripts": [
        {
            "css": [
                "styles/freeze-main.css"
            ],
            "matches": [
                "https://*.atlassian.net/browse/*"
            ]
        },
        {
            "css": [
                "styles/freeze-popup.css"
            ],
            "matches": [
                "https://*.atlassian.net/jira/software/*"
            ]
        }
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}